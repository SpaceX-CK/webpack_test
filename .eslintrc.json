{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": ["error", "windows"],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    },
    "ignorePatterns": ["node_modules","build"]
    // ,
    // "eslintIgnore": [
    //     "node_modules",
    //     "build"
    // ]
}
    // npx eslint --init

